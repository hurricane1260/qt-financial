<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qtjf.web.mapper.QtFinacialAuthenticationIdentificationMapper" >
  <resultMap id="BaseResultMap" type="com.qtjf.common.bean.QtFinacialAuthenticationIdentification" >
    <result column="user_id" property="userId" jdbcType="INTEGER" />
    <result column="front" property="front" jdbcType="VARCHAR" />
    <result column="face" property="face" jdbcType="VARCHAR" />
    <result column="type" property="type" jdbcType="INTEGER" />
  </resultMap>
  <insert id="insert" parameterType="com.qtjf.common.bean.QtFinacialAuthenticationIdentification" >
    insert into qt_finacial_authentication_identification (user_id, front, face, 
      type)
    values (#{userId,jdbcType=INTEGER}, #{front,jdbcType=VARCHAR}, #{face,jdbcType=VARCHAR}, 
      #{type,jdbcType=INTEGER})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap" >
    select user_id, front, face, type
    from qt_finacial_authentication_identification
  </select>
</mapper>